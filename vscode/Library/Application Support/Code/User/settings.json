    "multiCommand.commands": [
        {
            "command": "multiCommand.harpoonAddWithNotification",
            "sequence": [
                "vscode-harpoon.addEditor",
                {
                    "command": "editor.action.showHover",
                    "args": "File added to Harpoon"
                }
            ]
        }
    ],
